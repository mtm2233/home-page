{
  "name": "home-page",
  "version": "0.0.0",
  "scripts": {
    "dev": "node color && vite",
    "build": "node color && vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "dev-staging": "cross-env VUE_APP_MODE=staging vite",
    "build-staging": "node color && vue-tsc --noEmit && cross-env VUE_APP_MODE=staging vite build",
    "dev-production": "cross-env VUE_APP_MODE=production vite",
    "build-production": "node color && vue-tsc --noEmit && cross-env VUE_APP_MODE=production vite build",
    "lint": "eslint --fix --ext .ts,vue src/ --no-error-on-unmatched-pattern && prettier -w -u ."
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "antd-theme-generator": "^1.2.10",
    "axios": "^0.21.4",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "vue": "^3.2.2",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.4",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-vue": "^1.8.1",
    "@vue/compiler-sfc": "^3.2.12",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-style-import": "^1.2.1",
    "vue-tsc": "^0.3.0"
  }
}
